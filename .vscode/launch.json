{
  "version": "0.2.0",
  "configurations": [
    {
      "preLaunchTask": "Build ESP32",
      "type": "probe-rs-debug",
      "request": "launch",
      "name": "ESP: Run w/ Debugger",
      "flashingConfig": {
        "flashingEnabled": true,
      },
      "chip": "esp32",
      "coreConfigs": [
        {
        "programBinary": "${workspaceFolder}/esp/target/xtensa-esp32-none-elf/debug/esp",
        "rttEnabled": true
        }
      ],
      "cwd": "${workspaceFolder}/esp",
      "consoleLogLevel": "Console"
    }
  ]
}